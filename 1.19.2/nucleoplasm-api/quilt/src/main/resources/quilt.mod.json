{
  "schema_version": 1,
  "mixin": [
    "nucleoplasm-api.mixins.json",
    "nucleoplasm-api-common.mixins.json"
  ],
  "quilt_loader": {
    "group": "${group}",
    "id": "nucleoplasm_api",
    "version": "${version}",
    "name": "Nucleoplasm Api Mod",
    "description": "${mod_description}",
    "authors": ["${org_name}"],
    "contact": {
      "sources": ""
    },
    "license": "${license}",
    "icon": "assets/nucleoplasm_api/icon.png",
    "intermediate_mappings": "net.fabricmc:intermediary",
    "environment": "*",
    "entrypoints": {
      "init": [
        "org.overrun.nucleoplasm.api.quilt.ExampleModQuilt"
      ]
    },
    "depends": [
      {
        "id": "quilt_loader",
        "version": "*"
      },
      {
        "id": "quilt_base",
        "version": "*"
      },
      {
        "id": "minecraft",
        "version": ">=1.19.2"
      },
      {
        "id": "architectury",
        "version": ">=6.2.43"
      }
    ]
  }
}